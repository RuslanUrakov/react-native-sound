{
  "include": ["src"],
  "compilerOptions": {
    "declaration": true,
    "importHelpers": true,
    "jsx": "react-native",
    "lib": ["es2017"],
    "moduleResolution": "node",
    "noUnusedLocals": true,
    "outDir": "lib",
    "strict": true,
    "suppressImplicitAnyIndexErrors": true,
    "target": "es5"
  }
}
